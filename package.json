    "flow-bin": "0.79.1",
    "flow-coverage-report": "^0.5.0",
    "jasmine-core": "^3.0.0",
    "karma": "^3.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-commonjs": "^1.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.13",
